{
  "name": "@juiceplus/myjuiceplus",
  "private": true,
  "version": "0.0.0",
  "author": "Hugo Jobling <hello@thisishugo.com> (https://thisishugo.com)",
  "contributors": [
    "Hugo Jobling <hello@thisishugo.com> (https://thisishugo.com)",
    "Josh Kelly <josh@monster.dev> (https://www.monster.dev)"
  ],
  "scripts": {
    "dev": "run-script-os",
    "dev:win32": "concurrently --kill-others \"next dev\" \"npm run generate:graphql:watch\"",
    "dev:darwin:linux": "pm2-dev pm2.config.js",
    "prebuild": "npm run i18n:all && npm run generate:graphql",
    "build": "next build",
    "start": "next start",
    "generate:graphql": "graphql-codegen",
    "generate:graphql:watch": "npm run generate:graphql -- --watch",
    "generate:icons": "svgr --icon --typescript --ignore-existing -d src/components/ui/icons src/icons",
    "postgenerate:icons": "npm run eslint:fix",
    "i18n:compile": "formatjs compile-folder --ast --format crowdin src/lib/i18n/translations src/lib/i18n/translations/compiled",
    "i18n:extract": "formatjs extract --out-file src/lib/i18n/translations/en.json --id-interpolation-pattern \"[sha512:contenthash:12]\" --format crowdin \"src/**/*.{ts,tsx}\"",
    "i18n:all": "npm run i18n:extract && npm run i18n:compile",
    "prettier": "prettier \"**/*.{ts,tsx,js,json,css,md,html,yml,graphql}\"",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "eslint": "eslint . --ext ts,tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "jest --config jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "typecheck": "tsc --noEmit --project src",
    "typecheck:e2e": "tsc --noEmit --project e2e"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "classnames": "^2.3.1",
    "cookie": "^0.4.1",
    "date-fns": "^2.27.0",
    "graphql": "^16.1.0",
    "graphql-request": "^3.7.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "next": "^12.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.23.0",
    "run-script-os": "^1.1.6",
    "set-cookie-parser": "^2.4.8",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.5.0",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-graphql-request": "^4.3.2",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@svgr/cli": "^6.1.1",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/jit": "^0.1.18",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/date-fns": "^2.6.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/set-cookie-parser": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.1",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-formatjs": "^2.19.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "faker": "^5.5.3",
    "fishery": "^2.1.0",
    "jest": "^27.4.3",
    "jest-playwright-preset": "^1.5.2",
    "msw": "^0.36.2",
    "playwright": "1.10",
    "playwright-testing-library": "^4.0.1",
    "pm2": "^5.1.2",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "^2.2.19",
    "ts-jest": "^27.1.1",
    "typescript": "4.5.2"
  },
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "sideEffects": false
}
